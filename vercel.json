{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
        {
          "src": "/setsearchterm",
          "dest": "index.js"
        },
        {
          "src": "/animalname",
          "dest": "index.js"
        },
        {
          "src": "/animalimage",
          "dest": "index.js"
        },
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
    ]
}